    <!--新增题目-->
    <SQL SQLKey="MockTest.InsertQustion" ConnectionKey="MySql">
      <Text>
        <![CDATA[
INSERT t_exam_question
        ( content ,
          mode ,
          document_id ,
          img_url ,
          is_ip_restrict ,
          is_user_restrict ,
          order_num ,
          is_deleted ,
          create_user ,
          create_time,
		  user_tag,
		  answer_analysis
        ) VALUES 
                (
				  @content ,
          @mode ,
          @document_id ,
          @img_url ,
          @is_ip_restrict ,
          @is_user_restrict ,
          @order_num ,
          0 ,
          @create_user ,
          @create_time,
		  @user_tag,
		  @answer_analysis
                );
select LAST_INSERT_ID()  as id;
         ]]>
      </Text>
    </SQL>
